\n[0m[31m<a target=[1;35m\"[0m[31m_blank[1;35m\"[0m[31m href=[1;35m\"[0m[31m/pdfs/CS11694_The_Revised_Consumer_Protection_Indicator_Methods_report_DIGITAL.pdf[1;35m\"[0m[31m>The Revised Consumer Protection Indicator Methods Report</a><br>[1;35m\n[0m[31m<a target=[1;35m\"[0m[31m_blank[1;35m\"[0m[31m href=[1;35m\"[0m[31m/pdfs/OSH.Indicator.Methods.Final-DIGITAL.pdf[1;35m\"[0m[31m>OSH Indicator Methods Report</a><br><br>[1;35m\n[0m[31m[1;35m\n[0m[31m<style>[1;35m\n[0m[31m[1;35m\n[0m[31m[1;35m\n[0m[31m    #expand {[1;35m\n[0m[31m        height: 0;[1;35m\n[0m[31m        overflow: hidden;[1;35m\n[0m[31m        transition: height 0.5s;[1;35m\n[0m[31m        color: #FFF;[1;35m\n[0m[31m    }[1;35m\n[0m[31m    section {[1;35m\n[0m[31m        padding: 0 20px;[1;35m\n[0m[31m    }[1;35m\n[0m[31m    #toggle {[1;35m\n[0m[31m        font-weight:bold;[1;35m\n[0m[31m    }[1;35m\n[0m[31m    #toggle:checked ~ #expand {[1;35m\n[0m[31m        height: 400px;[1;35m\n[0m[31m    }[1;35m\n[0m[31m</style>[1;35m\n[0m[31m<input id=[1;35m\"[0m[31mtoggle[1;35m\"[0m[31m type=[1;35m\"[0m[31mcheckbox[1;35m\"[0m[31m style=[1;35m\"[0m[31mdisplay:none[1;35m\"[0m[31m>[1;35m\n[0m[31m<label for=[1;35m\"[0m[31mtoggle[1;35m\"[0m[31m style=[1;35m\"[0m[31mcursor:pointer;text-decoration:underline;color:#dc0012[1;35m\"[0m[31m>Downloads</label><br><br>[1;35m\n[0m[31m[1;35m\n[0m[31m<style>[1;35m\n[0m[31m    .mktoForm {[1;35m\n[0m[31m        width:100% !important;[1;35m\n[0m[31m    }[1;35m\n[0m[31m    .mktoFormRow, .mktoFieldWrap, .mktoFormCol, .mktoButtonRow {[1;35m\n[0m[31m        width:100%;[1;35m\n[0m[31m    }[1;35m\n[0m[31m    .mktoForm input[type=url],[1;35m\n[0m[31m    .mktoForm input[type=text],[1;35m\n[0m[31m    .mktoForm input[type=date],[1;35m\n[0m[31m    .mktoForm input[type=tel],[1;35m\n[0m[31m    .mktoForm input[type=email],[1;35m\n[0m[3